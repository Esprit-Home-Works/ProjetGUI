<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import java.net.URL?>
<AnchorPane prefHeight="200.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.project.guiproject.controllers.TeamPlayers.AddTeamController">

    <stylesheets>
        <URL value="@styles.css" />
    </stylesheets>
    <children>
        <HBox layoutX="20.0" layoutY="20.0" spacing="10.0">
            <children>
                <Label text="Select Team:" />
                <ComboBox fx:id="teamComboBox" prefWidth="150.0" />
            </children>
        </HBox>
        <Button fx:id="delete" contentDisplay="BOTTOM" layoutX="102.0" layoutY="74.0" onAction="#deleteTeam" prefHeight="34.0" prefWidth="137.0" styleClass="button button-primary" text="Delete Team" textFill="#a43838" />
    </children>
</AnchorPane>
