<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane prefHeight="200.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.project.guiproject.controllers.TeamPlayers.AddTeamController">
    <children>
        <HBox layoutX="20.0" layoutY="20.0" spacing="10.0">
            <children>
                <Label text="Select Team:" />
                <ComboBox fx:id="teamComboBox" prefWidth="150.0" />
            </children>
        </HBox>
        <Button fx:id="delete" styleClass="button button-primary" prefWidth="120.0" onAction="#deleteTeam" text="Delete Team" textFill="WHITE" />
    </children>
</AnchorPane>
