<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.project.guiproject.controllers.TeamPlayers.TeamController">
    <ImageView fx:id="background" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
    </ImageView>
    <Button onAction="#openAddTeamView" text="Open Add Team View" />
    <AnchorPane style="-fx-background-color: white;" AnchorPane.bottomAnchor="50.0" AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="50.0" AnchorPane.topAnchor="50.0">
        <Label text="Gestion des teams" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0">
            <font>
                <Font name="Tahoma Bold" size="36.0" />
            </font>
        </Label>
        <TableView fx:id="TeamsList" layoutY="150.0" onMouseClicked="#clickItem" prefHeight="110.0" prefWidth="500.0" AnchorPane.bottomAnchor="40.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="150.0">
            <columns>
                <TableColumn fx:id="teamName" minWidth="-Infinity" prefWidth="150.0" text="Name" />
                <TableColumn fx:id="teamDescription" minWidth="-Infinity" prefWidth="150.0" text="Description" />
                <TableColumn fx:id="teamLocation" minWidth="-Infinity" prefWidth="150.0" text="Location" />

            </columns>
        </TableView>
    </AnchorPane>
    <stylesheets>
        <URL value="@styles.css" />
    </stylesheets>

</AnchorPane>
